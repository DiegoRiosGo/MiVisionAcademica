{
    "builds" : [
        {
            "src": "mi_vision_academica/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.13.7"
            }
        }
        
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mi_vision_academica/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "staticfiles/$1"
        }
    ]
}